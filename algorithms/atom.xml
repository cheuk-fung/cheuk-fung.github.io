---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Algorithms -- {{ site.name }}</title>
    <link href="http://{{ site.link }}/algorithms/atom.xml" rel="self"/>
    <link href="http://{{ site.link }}/algorithms"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://{{ site.link }}/algorithms</id>
    <author>
        <name>{{ site.author }}</name>
        <email>{{ site.email }}</email>
    </author>

    {% for post in site.categories.algorithms limit:10 %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://{{ site.link }}{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>id:{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>
